{
  "name": "@zadkiel/gulp-feed",
  "version": "0.0.5",
  "private": false,
  "description": "Gulp `feed` wrapper module",
  "license": "MIT",
  "author": "Zadkiel <hello@zadkiel.fr>",
  "repository": "https://github.com/aslafy-z/gulp-feed",
  "keywords": [
    "gulpplugin",
    "feed",
    "rss",
    "atom",
    "stream"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && istanbul cover --include-all-sources _mocha tests --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "fancy-log": "^1.3.2",
    "feed": "^3.0.0",
    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.4",
    "eslint": "5.16.0",
    "husky": "1.1.4",
    "isstream": "0.1.2",
    "istanbul": "0.4.5",
    "lint-staged": "8.0.4",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "mockery": "2.1.0",
    "sinon": "6.3.5",
    "sinon-chai": "3.3.0",
    "standard": "12.0.1"
  }
}
